variable	sname index h_atom
log		${sname}.spe.log

units 		electron
newton		on
boundary        f f f

atom_style	electron

read_data  	data.${sname}

pair_style      eff/cut 500.0 0 0
pair_coeff 	* *

communicate	single vel yes

compute		effTemp all temp/eff

thermo		1
thermo_style	multi
thermo_modify	temp effTemp

run 		0
